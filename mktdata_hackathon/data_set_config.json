        "sample_ticker_data": {
          "table_name": "sample_ticker_data",
          "description": "Historical 30-year stock market data (OHLCV, possibly with splits and dividends)",
          "schema": [
            {
              "name": "date",
              "type": "DATE",
              "mode": "REQUIRED",
              "description": "Date of the market data"
            },
            {
              "name": "symbol",
              "type": "STRING",
              "mode": "REQUIRED",
              "description": "Trading symbol of the stock"
            },
            {
              "name": "open",
              "type": "FLOAT",
              "mode": "NULLABLE",
              "description": "Opening price"
            },
            {
              "name": "high",
              "type": "FLOAT",
              "mode": "NULLABLE",
              "description": "Highest price during the trading day"
            },
            {
              "name": "low",
              "type": "FLOAT",
              "mode": "NULLABLE",
              "description": "Lowest price during the trading day"
            },
            {
              "name": "close",
              "type": "FLOAT",
              "mode": "NULLABLE",
              "description": "Closing price"
            },
            {
              "name": "volume",
              "type": "INTEGER",
              "mode": "NULLABLE",
              "description": "Volume of shares traded"
